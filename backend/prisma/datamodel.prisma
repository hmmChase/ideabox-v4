type User @db(name: "user") {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: Float
}

type Idea @db(name: "idea") {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  idea: String!
}
